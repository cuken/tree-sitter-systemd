{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "systemd",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_definition"
          }
        }
      ]
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "directive"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "blank_line"
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "common_section"
        },
        {
          "type": "SYMBOL",
          "name": "service_section"
        },
        {
          "type": "SYMBOL",
          "name": "mount_section"
        },
        {
          "type": "SYMBOL",
          "name": "automount_section"
        },
        {
          "type": "SYMBOL",
          "name": "socket_section"
        },
        {
          "type": "SYMBOL",
          "name": "timer_section"
        },
        {
          "type": "SYMBOL",
          "name": "path_section"
        },
        {
          "type": "SYMBOL",
          "name": "slice_section"
        },
        {
          "type": "SYMBOL",
          "name": "swap_section"
        },
        {
          "type": "SYMBOL",
          "name": "scope_section"
        },
        {
          "type": "SYMBOL",
          "name": "container_section"
        },
        {
          "type": "SYMBOL",
          "name": "pod_section"
        },
        {
          "type": "SYMBOL",
          "name": "image_section"
        },
        {
          "type": "SYMBOL",
          "name": "volume_section"
        },
        {
          "type": "SYMBOL",
          "name": "network_section"
        }
      ]
    },
    "common_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Unit"
        },
        {
          "type": "STRING",
          "value": "Install"
        }
      ]
    },
    "service_section": {
      "type": "STRING",
      "value": "Service"
    },
    "mount_section": {
      "type": "STRING",
      "value": "Mount"
    },
    "automount_section": {
      "type": "STRING",
      "value": "Automount"
    },
    "socket_section": {
      "type": "STRING",
      "value": "Socket"
    },
    "timer_section": {
      "type": "STRING",
      "value": "Timer"
    },
    "path_section": {
      "type": "STRING",
      "value": "Path"
    },
    "slice_section": {
      "type": "STRING",
      "value": "Slice"
    },
    "swap_section": {
      "type": "STRING",
      "value": "Swap"
    },
    "container_section": {
      "type": "STRING",
      "value": "Container"
    },
    "pod_section": {
      "type": "STRING",
      "value": "Pod"
    },
    "image_section": {
      "type": "STRING",
      "value": "Image"
    },
    "volume_section": {
      "type": "STRING",
      "value": "Volume"
    },
    "network_section": {
      "type": "STRING",
      "value": "Network"
    },
    "scope_section": {
      "type": "STRING",
      "value": "Scope"
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "key": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9_]+"
    },
    "value": {
      "type": "PATTERN",
      "value": ".*"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "[\\s\\t]+"
    },
    "whitespace": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}